{
  "model": {
    "name": "mlp_single",
    "norm": "none",
    "dense": "dense",
    "auxiliary_net": false,
    "user_dim": 8,
    "item_dim": 8,
    "domain_dim": 8,
    "auxiliary_dim": 128,
    "hidden_dim": [
      256,
      128,
      64
    ],
    "dropout": 0.5
  },
  "train": {
    "load_pretrain_emb": false,
    "emb_trainable": true,
    "epoch": 99999,
    "learning_rate": 0.001,
    "result_save_path": "result",
    "checkpoint_path": "checkpoint",
    "loss": "binary_crossentropy",
    "optimizer": "adam",
    "patience": 3,
    "histogram_freq": 0,
    "shuffle_buff_size": 10000
  },
  "dataset": {
    "name": "Movielens",
    "dataset_path": "dataset/Movielens",
    "domain_split_path": "split_by_gender",
    "batch_size": 1024,
    "shuffle_buffer_size": 10000,
    "num_parallel_reads": 8,
    "seed": 123
  }
}